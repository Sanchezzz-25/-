meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            "РОФЛ": "шутка",
            "ЩИЩ": "одобрение или восторг",
            "КРИПОВЫЙ": "страшный, пугающий",
            "АГРИТЬСЯ": "злиться",
            "14ти летний уникал": "чел у которого большое эго",
            "Хардлайн": "верхняя линия сил света или нижняя линия сил тьмы",
            "Изилайн": "нижняя линия сил света или верхняя линия сил тьмы",
            "МИД": "центральная линия"
            }
while True:
    word = input("Введите непонятное слово (большими буквами!): ")
    if word in meme_dict.keys():
        print(meme_dict[word])
    else:
        print("Такого слова у нас нет")

        
